<li class="item" data-id="<%= item.escape('uuid') %>">
  <a class="collapser"></a>
  <a class="bullet"></a>

  <%= item.escape('title') %>
  <%= item.shortened_notes() %>

  <% if (!item.get('collapsed')) { %>
    <section class="indented">
      <ul class="list"></ul>
    </section>
  <% } %>
</li>